<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="bottom"
            type="com.m2comm.kses2019s_con.views.Bottom" />
    </data>

    <com.m2comm.kses2019s_con.modules.customview.CustomFrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.m2comm.kses2019s_con.modules.customview.CustomView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            app:h="60">


            <com.m2comm.kses2019s_con.modules.customview.CustomView
                android:id="@+id/home"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="1"
                >

                <com.m2comm.kses2019s_con.modules.customview.CustomImgView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_horizontal"
                    app:img="@drawable/bottomicon01"
                    app:w="25"
                    app:h="40"
                    />

                <com.m2comm.kses2019s_con.modules.customview.CustomTextView
                    android:layout_marginTop="-10dp"
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:text="메인"
                    app:fontType="condensed"
                    android:textColor="@color/main_color_black"
                    app:fontSize="14"
                    />

            </com.m2comm.kses2019s_con.modules.customview.CustomView>



            <com.m2comm.kses2019s_con.modules.customview.CustomView
                android:id="@+id/program"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="1"
                >

                <com.m2comm.kses2019s_con.modules.customview.CustomImgView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_horizontal"
                    app:img="@drawable/bottomicon02"
                    app:w="25"
                    app:h="40"
                    />

                <com.m2comm.kses2019s_con.modules.customview.CustomTextView
                    android:layout_marginTop="-10dp"
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:text="전체보기"
                    app:fontType="condensed"
                    android:textColor="@color/main_color_black"
                    app:fontSize="14"
                    />

            </com.m2comm.kses2019s_con.modules.customview.CustomView>


            <com.m2comm.kses2019s_con.modules.customview.CustomView
                android:id="@+id/now"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="1"
                >

                <com.m2comm.kses2019s_con.modules.customview.CustomImgView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_horizontal"
                    app:img="@drawable/bottomicon03"
                    app:w="40"
                    app:h="40"
                    />

                <com.m2comm.kses2019s_con.modules.customview.CustomTextView
                    android:layout_marginTop="-10dp"
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:text="진행세션"
                    app:fontType="condensed"
                    android:textColor="@color/main_color_black"
                    app:fontSize="14"
                    />

            </com.m2comm.kses2019s_con.modules.customview.CustomView>


            <com.m2comm.kses2019s_con.modules.customview.CustomView
                android:id="@+id/search"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="1"
                >

                <com.m2comm.kses2019s_con.modules.customview.CustomImgView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_horizontal"
                    app:img="@drawable/bottomicon04"
                    app:w="25"
                    app:h="40"
                    />

                <com.m2comm.kses2019s_con.modules.customview.CustomTextView
                    android:layout_marginTop="-10dp"
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:text="검색"
                    app:fontType="condensed"
                    android:textColor="@color/main_color_black"
                    app:fontSize="14"
                    />
            </com.m2comm.kses2019s_con.modules.customview.CustomView>

            <com.m2comm.kses2019s_con.modules.customview.CustomView
                android:id="@+id/fav"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="1"
                >

                <com.m2comm.kses2019s_con.modules.customview.CustomImgView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_horizontal"
                    app:img="@drawable/bottomicon05"
                    app:w="25"
                    app:h="40"
                    />

                <com.m2comm.kses2019s_con.modules.customview.CustomTextView
                    android:layout_marginTop="-10dp"
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:text="즐겨찾기"
                    app:fontType="condensed"
                    android:textColor="@color/main_color_black"
                    app:fontSize="14"
                    />

            </com.m2comm.kses2019s_con.modules.customview.CustomView>

        </com.m2comm.kses2019s_con.modules.customview.CustomView>

        <com.m2comm.kses2019s_con.modules.customview.CustomView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:h="1"
            android:layout_gravity="top"
            android:background="#071B35"
            />

    </com.m2comm.kses2019s_con.modules.customview.CustomFrameLayout>




</layout>